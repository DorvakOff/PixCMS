<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="Start Application" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot">
        <module name="PixCMS"/>
        <extension name="net.ashald.envfile">
            <option name="IS_ENABLED" value="false"/>
            <option name="IS_SUBST" value="false"/>
            <option name="IS_PATH_MACRO_SUPPORTED" value="false"/>
            <option name="IS_IGNORE_MISSING_FILES" value="false"/>
            <option name="IS_ENABLE_EXPERIMENTAL_INTEGRATIONS" value="false"/>
            <ENTRIES>
                <ENTRY IS_ENABLED="true" PARSER="runconfig"/>
            </ENTRIES>
        </extension>
        <option name="SPRING_BOOT_MAIN_CLASS" value="fr.pixteam.pixcms.Application"/>
        <option name="ALTERNATIVE_JRE_PATH"/>
        <method v="2">
            <option name="RunConfigurationTask" enabled="true" run_configuration_name="Build node"
                    run_configuration_type="js.build_tools.npm"/>
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>